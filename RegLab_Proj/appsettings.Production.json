{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "RedisSettings": {
    "InstanceName": "prod",
    "SettingCacheDuration": 30
  },
  "ConnectionStrings": {
    "MongoDB": "mongodb://prod_user:prod_password@prod.mongo.com:27017/user_settings?authSource=user_settings",
    "Redis": "prod.redis.com,abortConnect=False"
  },
  "MongoDBSettings": {
    "Database": "user_settings"
  },
  "Swagger": {
    "path": "/swagger/v1/swagger.json",
    "name": "SettingsServiceAPI",
    "versionAPI": "v1"
  },
  "AllowedHosts": "*"
}